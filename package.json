{
  "name": "@hostaqui/pdf-maker",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "fix": "npm run lint:fix && npm run prettier:fix",
    "release": "semantic-release",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test --watchAll",
    "clean": "rimraf .tarball coverage dist typings",
    "clean:build": "rimraf dist typings",
    "lint": "eslint ./ --ext js,jsx,ts",
    "lint:fix": "eslint ./ --ext js,jsx,ts --fix",
    "prettier": "prettier --check ./**/*.{js,jsx,ts}",
    "prettier:fix": "prettier --write ./**/*.{js,jsx,ts}",
    "build": "run-s build:{check,types,cjs,esm}",
    "prebuild": "npm run clean:build",
    "build:check": "tsc --noEmit ",
    "build:types": "tsc -p build/tsconfig.types.json",
    "build:cjs": "node build/cjs",
    "build:esm": "node build/esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HOSTAQUI/pdf.maker.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "typings"
  ],
  "keywords": [
    "react",
    "pdf",
    "maker",
    "handlebars",
    "puppeteer"
  ],
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "handlebars": "4.7.7"
  },
  "peerDependenciesMeta": {
    "handlebars": {
      "optional": true
    }
  },
  "dependencies": {
    "puppeteer": "10.1.0"
  },
  "devDependencies": {
    "@jest/globals": "27.0.6",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@types/jest": "27.0.0",
    "@types/jest-image-snapshot": "4.3.1",
    "@types/node": "16.9.1",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "conventional-changelog-angular": "5.0.13",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "esbuild": "0.12.26",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "4.0.0",
    "handlebars": "4.7.7",
    "jest": "27.0.6",
    "jest-image-snapshot": "4.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.0",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.7",
    "ts-jest": "27.0.3",
    "typescript": "4.9.4"
  }
}
